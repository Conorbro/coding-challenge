<h1>Tags Table</h1>

<table>
  <tr>
    <th>Id</th>
    <th>Name</th>
  </tr>

  <% @tags.each do |tag| %>
    <tr>
      <td><%= tag.id %></td>
      <td><%= tag.name %></td>
    </tr>
  <% end %>
</table>

<br>
<p><a href="http://localhost:3000/users">User's Table</a></p>
<p><a href="http://localhost:3000/user_tags">User Tag Association Table</a></p>
